{
  "name": "investors-exchange.api",
  "version": "0.1.0",
  "description": "Unofficial Browser / NodeJS API for https://iextrading.com",
  "main": "index.js",
  "author": "Arman Charan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/armancharan/investors-exchange.api"
  },
  "scripts": {
    "build": "yarn run clean && yarn run bundle",
    "bundle": "BABEL_ENV=production babel source --out-dir distribution --ignore **/*.test.js",
    "clean": "yarn run remove:distribution && yarn run create:distribution",
    "create:distribution": "mkdir distribution",
    "upload": "yarn run build && yarn publish",
    "remove:distribution": "rm -rf distribution",
    "test": "yarn jest --runInBand"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0-alpha.3cc09dcf",
    "babel-preset-stage-3": "^6.24.1",
    "jest": "^23.6.0",
    "path": "^0.12.7",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.21.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
